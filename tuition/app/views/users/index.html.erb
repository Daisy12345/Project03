<section class="content-wrapper">
<aside class="md-col md-col-3">
  <div class="side-bar">
  <ul class="nav nav-pills nav-stacked labels-info inbox-divider">
        <li class="highlight"><h4>Classes</h4></li>
        <li><%= link_to "Private classes", personalclasses_path %></li>
        <li><%= link_to 'Students', users_path %></li>
        <li><%= link_to "workshops", workshops_path %></li>
  </ul>
  </div>
</aside>

<div class="sidebar md-col md-col-9">
<div class="table-box">
<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>E_mail</th>
      <th>User Id</th>
   <!--    <th>Photo</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.phone %></td>
        <td><%= user.email %></td>
        <td><%= user.id %></td>
      <!--   <td><%= image_tag(user.photo) %></td> -->
        
        <% if user_signed_in? %>
       <!--  <td><%= link_to 'Add photo', "" %></td> -->
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
 
 <!-- <%= link_to 'New User', new_user_path %> -->

</div>
</div>
</section>